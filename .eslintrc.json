{
  "env": {
	"browser": true,
	"es6": true
  },
  "extends": [
	"standard",
	"standard-preact",
	"plugin:jest/recommended"
  ],
  "globals": {
	"Atomics": "readonly",
	"SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
	"ecmaFeatures": {
	  "jsx": true
	},
	"ecmaVersion": 9,
	"sourceType": "module"
  },
  "plugins": [
	"react",
	"jest"
  ],
  "rules": {
	"dot-notation": "off"
  }
}
